# Environment Variables
# Kopirajte ovaj fajl u .env i popunite vrednosti

# Google Gemini API Key
# Dobijte ga na: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_api_key_here

# Server Configuration
PORT=3000
NODE_ENV=development

# CORS Configuration (dozvoljena domena va≈°eg sajta)
ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000,https://olympic.rs

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=20
